{
    "type": "project",
    "name": "archlinux-de/pkgstats.archlinux.de",
    "description": "Arch Linux package statistics website",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-apcu": "^5.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "doctrine/doctrine-bundle": "^2.1.0",
        "doctrine/doctrine-migrations-bundle": "^2.1.2",
        "doctrine/orm": "^2.7.3",
        "maxmind-db/reader": "^1.6.0",
        "nelmio/api-doc-bundle": "^3.6.1",
        "sensio/framework-extra-bundle": "^5.5.5",
        "symfony/asset": "^5.1.0",
        "symfony/console": "^5.1.0",
        "symfony/dotenv": "^5.1.0",
        "symfony/expression-language": "^5.1.0",
        "symfony/flex": "^1.7.1",
        "symfony/framework-bundle": "^5.1.0",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/twig-bundle": "^5.1.0",
        "symfony/validator": "^5.1.0",
        "symfony/web-link": "^5.1.0",
        "symfony/webpack-encore-bundle": "^1.7.3",
        "symfony/yaml": "^5.1.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "malukenho/mcbumpface": "^1.1.2",
        "phpstan/phpstan": "^0.12.25",
        "phpstan/phpstan-deprecation-rules": "^0.12.4",
        "phpstan/phpstan-doctrine": "^0.12.13",
        "phpstan/phpstan-phpunit": "^0.12.11",
        "phpstan/phpstan-symfony": "^0.12.6",
        "phpunit/phpunit": "^9.2.1",
        "pierres/doctrine-migrations-test": "^1.0.4",
        "pierres/symfony-database-test": "^1.0.6",
        "sensiolabs/security-checker": "^6.0.3",
        "squizlabs/php_codesniffer": "^3.5.5",
        "symfony/browser-kit": "^5.1.0",
        "symfony/css-selector": "^5.1.0",
        "symfony/phpunit-bridge": "^5.1.0",
        "symfony/stopwatch": "^5.1.0",
        "symfony/web-profiler-bundle": "^5.1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        },
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    }
}
