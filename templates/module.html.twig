{% extends 'base.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('build/js/module.js') }}"></script>
{% endblock %}

{% block title %}Module statistics{% endblock %}

{% block body %}
    <h1 class="mb-4">Module statistics</h1>
    <table id="modules" class="table table-striped table-bordered table-sm" width="100%" cellspacing="0"
           data-ajax="{{ path('app_modulestatistics_datatables') }}">
        <thead>
        <tr>
            <th>Module</th>
            <th>Usage</th>
        </tr>
        </thead>
    </table>
{% endblock %}
