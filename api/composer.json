{
    "type": "project",
    "name": "archlinux-de/pkgstats.archlinux.de",
    "description": "Arch Linux package statistics website",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.2",
        "ext-apcu": "^5.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "ext-sysvsem": "*",
        "doctrine/doctrine-bundle": "^2.8.3",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.14.1",
        "maxmind-db/reader": "^1.11.0",
        "nelmio/api-doc-bundle": "^4.11.1",
        "symfony/console": "^6.2.7",
        "symfony/dotenv": "^6.2.7",
        "symfony/expression-language": "^6.2.7",
        "symfony/flex": "^2.2.5",
        "symfony/framework-bundle": "^6.2.7",
        "symfony/intl": "^6.2.7",
        "symfony/lock": "^6.2.7",
        "symfony/monolog-bundle": "^3.8.0",
        "symfony/property-info": "^6.2.7",
        "symfony/rate-limiter": "^6.2.7",
        "symfony/runtime": "^6.2.7",
        "symfony/serializer": "^6.2.7",
        "symfony/twig-bundle": "^6.2.7",
        "symfony/validator": "^6.2.7",
        "symfony/yaml": "^6.2.7"
    },
    "require-dev": {
        "ext-libxml": "*",
        "ext-pdo_sqlite": "*",
        "ext-simplexml": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.4.2",
        "league/iso3166": "^4.2.1",
        "malukenho/mcbumpface": "^1.2.0",
        "phpstan/extension-installer": "^1.2.0",
        "phpstan/phpstan": "^1.10.3",
        "phpstan/phpstan-deprecation-rules": "^1.1.2",
        "phpstan/phpstan-doctrine": "^1.3.33",
        "phpstan/phpstan-phpunit": "^1.3.9",
        "phpstan/phpstan-symfony": "^1.2.23",
        "phpunit/phpunit": "^10.0.14",
        "pierres/doctrine-migrations-test": "^2.0.0",
        "pierres/symfony-database-test": "^2.0.0",
        "spatie/phpunit-snapshot-assertions": "^5.0.1",
        "squizlabs/php_codesniffer": "^3.7.2",
        "symfony/phpunit-bridge": "^6.2.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "malukenho/mcbumpface": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*",
            "docker": false
        },
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    }
}
