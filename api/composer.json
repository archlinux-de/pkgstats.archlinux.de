{
    "type": "project",
    "name": "archlinux-de/pkgstats.archlinux.de",
    "description": "Arch Linux package statistics website",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-apcu": "^5.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "doctrine/doctrine-bundle": "^2.1.0",
        "doctrine/doctrine-migrations-bundle": "^3.0.1",
        "doctrine/orm": "^2.7.3",
        "maxmind-db/reader": "^1.7.0",
        "nelmio/api-doc-bundle": "^3.6.1",
        "sensio/framework-extra-bundle": "^5.5.6",
        "symfony/console": "^5.1.3",
        "symfony/dotenv": "^5.1.3",
        "symfony/expression-language": "^5.1.3",
        "symfony/flex": "^1.9.1",
        "symfony/framework-bundle": "^5.1.3",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/twig-bundle": "^5.1.3",
        "symfony/validator": "^5.1.3",
        "symfony/yaml": "^5.1.3"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "malukenho/mcbumpface": "^1.1.2",
        "phpstan/phpstan": "^0.12.37",
        "phpstan/phpstan-deprecation-rules": "^0.12.5",
        "phpstan/phpstan-doctrine": "^0.12.19",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-symfony": "^0.12.7",
        "phpunit/phpunit": "^9.3.7",
        "pierres/doctrine-migrations-test": "^1.1.1",
        "pierres/symfony-database-test": "^1.0.6",
        "sensiolabs/security-checker": "^6.0.3",
        "squizlabs/php_codesniffer": "^3.5.6",
        "symfony/phpunit-bridge": "^5.1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        },
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    }
}
